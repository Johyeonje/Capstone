<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.attendandroidapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="F-Attend"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/face"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true" >
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name="com.cookandroid.attendandroidapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cookandroid.attendandroidapp.adminActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.attend_courseActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.attendActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.loginActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.recordActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.computercourseActivity" />
        <activity android:name="com.cookandroid.attendandroidapp.myinfoActivity" />
    </application>

</manifest>